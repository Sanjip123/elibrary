@model elibrary.user_table 

@{
    ViewBag.Title = "Index";
}

<h2>WELCOME TO ADMIN LOGIN PAGE</h2>
<table>
    @{
    Html.BeginForm("Index","Admin",FormMethod.Post);
    <tr>
        <td>User name:</td>
        <td>@Html.TextBoxFor(a => a.user_name)</td>
    </tr>
    <tr>
        <td>Password:</td>
        <td>@Html.PasswordFor(a => a.user_password)</td>
    </tr>
    <tr>
        <td colspan="2"><input type="submit"value="login"/>
        </td>
    </tr>
    <tr>
        <td colspan="2">@ViewBag.asg</td>
    </tr>
    }
</table>

